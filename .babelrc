{
    "presets": [
        "stage-0",
        "env",
        "react",
    ],
    "plugins": [
        "react-hot-loader/babel", 
        "transform-object-rest-spread", 
        "transform-class-properties",
        [
            "transform-runtime", 
            {
                "polyfill": false,
                "regenerator": true
            }
        ]
    ],
    "sourceMap": "inline",
    "retainLines": true
}
